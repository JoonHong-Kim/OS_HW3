    "version": "0.2.0",
    "configurations": [
        {
            "name": "gcc - 활성 파일 빌드 및 디버그",
            "type": "cppdbg",
            "request": "launch",
            "program": "${fileDirname}//${fileBasenameNoExtension}",
            "targetArchitecture": "x86_64",
            "args": [
                "0",
                "10",
                "20",
                "./mtraces/gcc.trace"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "lldb",
            "preLaunchTask": "C/C++: gcc build active file"
        }
    ]
}